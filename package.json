{
    "name": "mulmocast-automation-system",
    "version": "1.0.0",
    "description": "Mulmocast-cliとOpenAI APIを使用した自動コンテンツ生成システム",
    "main": "mulmocast-automation.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "generate": "node mulmocast-automation.js",
      "setup": "npm install && npm run setup:env",
      "setup:env": "node -e \"require('fs').writeFileSync('.env', 'OPENAI_API_KEY=your-key-here\\nOPENAI_ORG_ID=your-org-id\\nPATH_BGM=./assets/music/bgm.mp3\\n')\"",
      "test": "node test-automation.js"
    },
    "keywords": [
      "mulmocast",
      "ai",
      "video-generation",
      "content-creation",
      "automation"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "axios": "^1.6.0",
      "cheerio": "^1.0.0-rc.12",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "multer": "^1.4.5-lts.1",
      "openai": "^4.20.0",
      "uuid": "^9.0.1"
    },
    "devDependencies": {
      "nodemon": "^3.0.1"
    },
    "engines": {
      "node": ">=18.0.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/mulmocast-automation.git"
    }
  }